#include <stdio.h>
void Max12(int n, int a[]){
    int Max1 = -1000000;
    int Max2 = -1000000;
    for(int i=0; i<n; i++){
        if(a[i]>=Max1){
            int temp = Max1;
            Max1 = a[i];
            Max2 = temp;
        }
        else if(a[i]>Max2 && a[i]<=Max1){
            Max2 = a[i];
        }
    }
    printf("Max1 = %d, Max2 = %d\n",Max1,Max2);// Max 1 có thể bằng Max 2
}

void MaX12(int n, int a[]){
    int Max1 = -1000000;
    int Max2 = -1000000;
    for(int i=0; i<n; i++){
        if(a[i]>Max1){
            int temp = Max1;
            Max1 = a[i];
            Max2 = temp;
        }
        else if(a[i]>Max2 && a[i]!=Max1){
            Max2 = a[i];
        }
    }
    if(Max2 == -1000000) printf("Max1 = %d, %d", Max1, -1);// nếu ko có Max2 thì in -1
    else printf("Max1 = %d, Max2 = %d\n",Max1,Max2);// Max 1 ko thể bằng Max 2
}

int main() {
    int n; scanf("%d",&n);
    int a[n];
    for(int i=0; i<n; i++){
        scanf("%d",&a[i]);
    }
    Max12(n,a);
    MaX12(n,a);
    return 0;
}
